@model IEnumerable<WebStore.Models.Employee>

    <div class="container">
    <table class="table">
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Surname</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td><a href="~/Home/Employee/@item.Id">Подробнее</a></td>
            </tr>
        }
    </table>
    </div>
